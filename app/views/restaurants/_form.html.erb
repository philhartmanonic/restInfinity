<%= form_for @restaurant do |f| %>
	<p><%= f.label :name %> <%= f.text_field :name %></p>
	<p><%= f.label :description %> <%= f.text_area :description %></p>
	<p><%= f.label :street %> <%= f.text_field :street %></p>
	<p><%= f.label :city %> <%= f.text_field :city %></p>
	<p><%= f.label :state %> <%= f.text_field :state %></p>
	<p><%= f.label :zip %> <%= f.text_field :zip %></p>
	<p><%= f.label :phone %> <%= f.text_field :phone %></p>
	<%= f.submit %></br>
<% end %>